doctype html
html
  head 
    link(rel='stylesheet', href='../../css/main.css')
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')
  body 
    include Custom-Navbar
    div.container
      div.c-dashboard1
        div.c-dashboard1__header
          div.c-dashboard1__header__heading 
            h1 Dashboard
          div.c-dashboard1__header__Button
            button.c-button 
              i.fa.fa-user-plus(aria-hidden='true')
                span   Invite users

        include 1-dashCardcomponent
        div.general
          div.c-geric-card 
            include custom-heading
            +custom-heading("Import data into Front Dashboard")

            span.he.all See and talk to your users and leads immediately
            include 1-dashboard-importantlist
            div.c-geric-card__bottom
              include Custom-dropdown
          div.c-geric-card 
            +custom-heading("Monthly expenses")
            div.c-geric-card__img
              img(src="../../images/chart.png")
            div.c-geric-card__bottom.left
              include Custom-dropdown
        div.table-user
          include 1-dash-custom-table
        div.c-trasction-repost
          div.c-geric-card
            +custom-heading("Transactions")
