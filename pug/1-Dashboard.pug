            div.c-geric-card__bottom
              include Custom-dropdown
          div.c-geric-card
            +custom-heading("Reports overview")
            +custom-heading("$7,432.14 USD")
            div.c-geric-card__report-progrss
              progress
              div.c-geric-card__report-persantage
